CONFIG_PACKAGE_luci-app-cifs-mount=y

# 打印服务器
# admin > nas > p910nd
# / 网络存储 / 打印服务器
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-p910nd
CONFIG_PACKAGE_luci-app-p910nd=y

# aMule设置
# admin > nas > amule
# / 网络存储 / aMule设置
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-amule
CONFIG_PACKAGE_luci-app-amule=y

# Aria2 配置
# admin > nas > aria2
# / 网络存储 / Aria2 配置
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-aria2
CONFIG_PACKAGE_luci-app-aria2=y

# BaiduPCS Web
# admin > nas > baidupcs-web
# / 网络存储 / BaiduPCS Web
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-baidupcs-web
CONFIG_PACKAGE_luci-app-baidupcs-web=y

# miniDLNA
# admin > nas > minidlna
# / 网络存储 / miniDLNA
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-minidlna
CONFIG_PACKAGE_luci-app-minidlna=y

# qBittorrent
# admin > nas > qBittorrent
# / 网络存储 / qBittorrent
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-qbittorrent
CONFIG_PACKAGE_luci-app-qbittorrent=y

# Rclone
# admin > nas > rclone
# / 网络存储 / Rclone
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-rclone
CONFIG_PACKAGE_luci-app-rclone=y

# Transmission
