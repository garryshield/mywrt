#### 内置
# 实时监控
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-netdata
CONFIG_PACKAGE_luci-app-netdata=y

# 负载均衡
# https://github.com/coolsnowwolf/luci/tree/master/applications/luci-app-mwan3
CONFIG_PACKAGE_luci-app-mwan3=y
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-mwan3helper
CONFIG_PACKAGE_luci-app-mwan3helper=y

# 释放内存
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-ramfree
CONFIG_PACKAGE_luci-app-ramfree=y

# Web 管理
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-webadmin
CONFIG_PACKAGE_luci-app-webadmin=y

# 磁盘管理
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-diskman
CONFIG_PACKAGE_luci-app-diskman=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_lsblk=y

# 多线多拨	
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-syncdial
CONFIG_PACKAGE_luci-app-syncdial=y

# Docker
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-docker
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-dockerman
CONFIG_PACKAGE_luci-app-docker=y
CONFIG_PACKAGE_luci-app-dockerman=y

# 实时流量监测
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-wrtbwmon
CONFIG_PACKAGE_luci-app-wrtbwmon=y

# SQM QoS
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-sqm
CONFIG_PACKAGE_luci-app-sqm=y

# 服务质量（QoS）
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-qos
CONFIG_PACKAGE_luci-app-qos=y

# Socat
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-socat
CONFIG_PACKAGE_luci-app-socat=y

# VPN
CONFIG_PACKAGE_luci-app-softethervpn=y
CONFIG_PACKAGE_luci-app-pptp-server=y
CONFIG_PACKAGE_luci-app-openvpn-server=y
CONFIG_PACKAGE_luci-app-ipsec-vpnd=y
CONFIG_PACKAGE_luci-app-zerotier=y

# Nas
CONFIG_PACKAGE_luci-app-amule=y
CONFIG_PACKAGE_luci-app-aria2=y
CONFIG_PACKAGE_luci-app-baidupcs-web=y
CONFIG_PACKAGE_luci-app-cifs-mount=y
CONFIG_PACKAGE_luci-app-hd-idle=y
CONFIG_PACKAGE_luci-app-kodexplorer=y
CONFIG_PACKAGE_luci-app-minidlna=y
CONFIG_PACKAGE_luci-app-nfs=y
CONFIG_PACKAGE_luci-app-p910nd=y
CONFIG_PACKAGE_luci-app-qbittorrent=y
CONFIG_PACKAGE_luci-app-rclone=y
CONFIG_PACKAGE_luci-app-samba=y
CONFIG_PACKAGE_luci-app-transmission=y
CONFIG_PACKAGE_luci-app-usb-printer=y
CONFIG_PACKAGE_luci-app-verysync==y

#### 第三方
CONFIG_PACKAGE_luci-app-advanced=y

CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_luci-app-adguardhome=y

# 应用过滤
CONFIG_PACKAGE_appfilter=y
CONFIG_PACKAGE_kmod-oaf=y
CONFIG_PACKAGE_luci-app-oaf=y