# 实时监控
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-netdata
CONFIG_PACKAGE_luci-app-netdata=y

# 负载均衡
# https://github.com/coolsnowwolf/luci/tree/master/applications/luci-app-mwan3
CONFIG_PACKAGE_luci-app-mwan3=y
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-mwan3helper
CONFIG_PACKAGE_luci-app-mwan3helper=y

# 释放内存
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-ramfree
CONFIG_PACKAGE_luci-app-ramfree=y

# Web 管理
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-webadmin
CONFIG_PACKAGE_luci-app-webadmin=y

# 磁盘管理
# https://github.com/coolsnowwolf/luci/blob/master/applications/luci-app-diskman
CONFIG_PACKAGE_luci-app-diskman=y
CONFIG_PACKAGE_luci-app-diskman_INCLUDE_lsblk=y

CONFIG_PACKAGE_luci-app-onliner=y
CONFIG_PACKAGE_luci-app-advanced=y
CONFIG_PACKAGE_luci-app-filebrowser=y
CONFIG_PACKAGE_luci-app-poweroff=y

CONFIG_PACKAGE_luci-app-openclash=y
CONFIG_PACKAGE_luci-app-adguardhome=y